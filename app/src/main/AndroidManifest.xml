<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vdopia.sdk21">
    <meta-data android:name="com.google.android.gms.version"
        android:value="@integer/google_play_services_version"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main2Activity" />
        <activity android:name=".Main3Activity"></activity>
        <activity android:name=".InviewListActivity"></activity>
        <meta-data android:name="app_license" android:value="1926ad5aa50f2bb1c05cd31fd3c9ad40"/>
        <provider android:name="com.duapps.ad.stats.DuAdCacheProvider"
            android:authorities="${applicationId}.DuAdCacheProvider"
            android:exported="false"> </provider>
    </application>

</manifest>